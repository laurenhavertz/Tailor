
<div class="container">
  <div id="carousel-example-generic" class="carousel slide">

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
      <div class="item active">
        <img src="/assets/black_dress.png">
        <div class="carousel-caption">
          <h3 class='top'>Style profiles</h3><h3 class='middle'> for bars</h3><h3 class='bottom'>around San Francisco</h3>
        </div>
      </div>
      <div class="item">
        <img src="http://johnathanclover.com/wp-content/uploads/galleries/post-216/Connor-021.jpg">
        <div class="carousel-caption">
          <h3 class='item-two-top'>The right clothes</h3><h3 class='item-two-bottom'> for the right places</h3>
        </div>
      </div>
      <div class="item">
         <img src="/assets/wang_dress.png" alt="900x600px">
         <div class="carousel-caption">
          <h3 class='item-three-top'>What to wear</h3> <h3 class='item-three-bottom'>wherever you go</h3>
         </div>
       </div>
    </div>

    <!-- Controls -->
    <a class="left carousel-control" href="#carousel-example-generic" data-slide="prev">
      <span class="icon-prev"></span>
    </a>
    <a class="right carousel-control" href="#carousel-example-generic" data-slide="next">
      <span class="icon-next"></span>
    </a>

    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
      <li data-target="#carousel-example-generic" data-slide-to="1"></li>
      <li data-target="#carousel-example-generic" data-slide-to="2"></li>
    </ol>

     <!-- Search Bar -->
     <div class="search">
       <div class='search-bar'>
         <%= form_tag(controller: "venues", action: "search", method: "get") do %>
         <%= text_field_tag(:venue, nil, placeholder: 'Where do you want to go?') %>
         <%= submit_tag("Search") %>
      </div>
       <br />
      <div class="search-params">
         <%= radio_button_tag(:gender, "man") %>
         <%= label_tag(:gender_male, "Man") %>
         <%= radio_button_tag(:gender, "woman") %>
         <%= label_tag(:gender_female, "Woman") %>
      </div>
       <% end %>
    </div>

  </div>
</div>